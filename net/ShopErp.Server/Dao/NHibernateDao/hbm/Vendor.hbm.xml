<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ShopErp.Domain" namespace="ShopErp.Domain">
  <class name="Vendor" table="vendor" lazy="false" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Name">
      <column name="Name" sql-type="string" not-null="true" />
    </property>
    <property name="PingyingName">
      <column name="PingyingName" sql-type="string" not-null="true" />
    </property>
    <property name="MarketAddress">
      <column name="MarketAddress" sql-type="string" not-null="true" />
    </property>
    <property name="HomePage">
      <column name="HomePage" sql-type="string" not-null="true" />
    </property>
    <property name="CreateTime">
      <column name="CreateTime" sql-type="datetime" not-null="true" />
    </property>
    <property name="Watch">
      <column name="Watch" sql-type="boolean" not-null="true" />
    </property>
    <property name="Count">
      <column name="Count" sql-type="int" not-null="true" />
    </property>
    <property name="AveragePrice">
      <column name="AveragePrice" sql-type="int" not-null="true" />
    </property>
    <property name="Comment">
      <column name="Comment" sql-type="string" not-null="true" />
    </property>
    <property name="Alias">
      <column name="Alias" sql-type="string" not-null="true" />
    </property>
    <property name="MarketAddressShort">
      <column name="MarketAddressShort" sql-type="string" not-null="true" />
    </property>
  </class>
</hibernate-mapping>