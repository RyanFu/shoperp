<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ShopErp.Domain" namespace="ShopErp.Domain">
  <class name="FinanceAccount" table="financeaccount" lazy="false" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Type">
      <column name="Type" sql-type="int" not-null="true" />
    </property>
    <property name="Name">
      <column name="Name" sql-type="string" not-null="true" />
    </property>
    <property name="Number">
      <column name="Number" sql-type="string" not-null="true" />
    </property>
    <property name="ShortInfo">
      <column name="ShortInfo" sql-type="string" not-null="true" />
    </property>
    <property name="Money">
      <column name="Money" sql-type="DECIMAL(22,2)" not-null="true" />
    </property>
    <property name="CreateTime">
      <column name="CreateTime" sql-type="datetime" not-null="true" />
    </property>
    <property name="Order">
      <column name="`Order`" sql-type="int" not-null="true" />
    </property>
  </class>
</hibernate-mapping>