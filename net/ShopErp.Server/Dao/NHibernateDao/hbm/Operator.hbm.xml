<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ShopErp.Domain" namespace="ShopErp.Domain">
  <class name="Operator" table="operator" lazy="false" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Number">
      <column name="Number" sql-type="string" not-null="true" />
    </property>
    <property name="Name">
      <column name="Name" sql-type="string" not-null="true" />
    </property>
    <property name="Password">
      <column name="Password" sql-type="string" not-null="true" />
    </property>
    <property name="Phone">
      <column name="Phone" sql-type="string" not-null="true" />
    </property>
    <property name="Rights">
      <column name="Rights" sql-type="string" not-null="true" />
    </property>
    <property name="CreateTime">
      <column name="CreateTime" sql-type="datetime" not-null="true" />
    </property>
    <property name="UpdateTime">
      <column name="UpdateTime" sql-type="datetime" not-null="true" />
    </property>
    <property name="CreateOperator">
      <column name="CreateOperator" sql-type="string" not-null="true" />
    </property>
    <property name="Enabled">
      <column name="Enabled" sql-type="boolean" not-null="true" />
    </property>
  </class>
</hibernate-mapping>